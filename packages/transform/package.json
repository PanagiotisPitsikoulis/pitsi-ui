{
  "name": "transform",
  "version": "0.0.1",
  "private": true,
  "description": "HTML to TSX and other format transformations",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "scrape": "tsx src/features/scraper/scraper.ts",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache"
  },
  "devDependencies": {
    "@types/cheerio": "^1.0.0",
    "@types/node": "^20.11.27",
    "tsup": "^8.5.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "p-limit": "^7.2.0"
  }
}
