{
  "name": "v4",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 4000",
    "build": "pnpm --filter=pitsi build && next build",
    "start": "next start --port 4000",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "registry:build": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts && prettier --log-level silent --write \"registry/**/*.{ts,tsx,json,mdx}\" --cache",
    "registry:capture": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/capture-registry.mts",
    "validate:registries": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/validate-registries.mts",
    "readiness": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/manage-readiness.mts",
    "readiness:list": "pnpm readiness list",
    "readiness:update": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/manage-readiness.mts update",
    "readiness:bulk": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/manage-readiness.mts bulk-update",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "stripe:test": "./scripts/stripe-test.sh",
    "stripe:setup": "./scripts/stripe-setup.sh",
    "images:webp": "bun ./scripts/convert-images-to-webp.ts",
    "postinstall": "fumadocs-mdx"
