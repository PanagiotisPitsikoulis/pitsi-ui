ath\n        d=\"M88.6 102.088C87.75 106.988 86.26 111.098 84.15 114.398C82.17 117.498 79.63 119.888 76.54 121.588L75.47 122.128C74.89 122.418 74.29 122.678 73.67 122.908C65.58 125.938 56.27 124.418 45.74 118.328C39.68 114.828 33.99 110.228 28.69 104.508C23.38 98.7981 18.76 92.5481 14.83 85.7681C10.9 78.9881 7.78 71.8781 5.47 64.4581C3.16 57.0381 2 49.8481 2 42.8981C2 30.8081 5.36 22.0781 12.07 16.7181C13.22 15.7981 14.43 15.0181 15.69 14.3781L16.49 13.9781C19.72 12.4781 23.3 11.8281 27.22 12.0381C30.49 12.2081 33.99 12.9781 37.73 14.3281C36.43 21.7781 36.83 30.0181 38.92 39.0581C41.01 48.0881 44.62 57.0281 49.74 65.8581C54.87 74.6981 60.84 82.2881 67.66 88.6281C74.48 94.9681 81.46 99.4581 88.61 102.088H88.6Z\"\n        stroke=\"currentColor\"\n      />\n      <path\n        d=\"M108.6 92.0881L88.5998 102.088C81.4498 99.4581 74.4698 94.9681 67.6498 88.6281C60.8298 82.2881 54.8598 74.6981 49.7298 65.8581C44.6098 57.0281 40.9998 48.0881 38.9098 39.0581C36.8198 30.0181 36.4198 21.7781 37.7198 14.3281L57.7198 4.32812C56.4198 11.7781 56.8198 20.0181 58.9098 29.0581C60.9998 38.0881 64.6098 47.0281 69.7298 55.8581C74.8598 64.6981 80.8298 72.2881 87.6498 78.6281C94.4698 84.9681 101.45 89.4581 108.6 92.0881Z\"\n        stroke=\"currentColor\"\n      />\n    </svg>\n  )\n}\n\nfunction EasingIcon({ className }: { className?: string }) {\n  return (\n    <svg\n      viewBox=\"0 0 115 112\"\n      fill=\"none\"\n      className={className}\n      strokeWidth=\"1\"\n      strokeLinecap=\"round\"\n      strokeLinejoin=\"round\"\n    >\n      <path\n        d=\"M91.8801 78.2502C91.3001 74.3002 90.2801 70.3203 88.8101 66.2903C87.4701 62.6103 85.7599 58.9003 83.6699 55.1503C83.3999 54.6603 83.1201 54.1703 82.8401 53.6803C80.5501 49.7303 78.09 46.1503 75.45 42.9403C70.72 37.1703 65.4201 32.5902 59.5601 29.2102C55.8101 27.0402 52.29 25.6302 49 24.9702C44.74 24.1002 40.8701 24.4903 37.4001 26.1603L36.8401 26.4403C36.6301 26.5403 36.43 26.6503 36.23 26.7703C36.11 26.8403 35.9801 26.9202 35.8601 27.0002C33.4101 28.4902 31.44 30.5103 29.98 33.0503C29.08 34.5803 28.3701 36.3102 27.8601 38.2202C27.6101 39.0802 27.41 39.9703 27.25 40.9103C26.86 43.0603 26.6699 45.4102 26.6699 47.9702C26.6699 48.0602 26.6699 48.1602 26.6699 48.2502C26.6699 48.8402 26.68 49.4403 26.7 50.0403C26.8 52.4403 27.06 54.8503 27.49 57.2803C27.97 60.0603 28.6701 62.8503 29.5901 65.6603C31.1601 70.4903 33.38 75.3502 36.22 80.2602C42.59 91.2502 50.37 99.4003 59.55 104.7C68.19 109.68 75.6101 110.68 81.8101 107.7L82.04 107.59C82.31 107.46 82.5701 107.32 82.8301 107.17C86.1001 105.33 88.5301 102.59 90.1301 98.9403C91.6701 95.4503 92.4399 91.1103 92.4399 85.9503C92.4399 83.4003 92.2501 80.8302 91.8701 78.2502H91.8801Z\"\n        stroke=\"currentColor\"\n      />\n      <path\n        d=\"M112.45 75.9504C112.45 86.4804 109.25 93.5603 102.84 97.1703C102.64 97.2803 102.44 97.3903 102.25 97.4903L101.85 97.6904L82.0503 107.59C82.3203 107.46 82.5803 107.32 82.8403 107.17C86.1103 105.33 88.5404 102.59 90.1404 98.9404C91.6804 95.4504 92.4502 91.1104 92.4502 85.9504C92.4502 83.4004 92.2604 80.8303 91.8804 78.2503C91.3004 74.3003 90.2803 70.3203 88.8103 66.2903C87.4703 62.6103 85.7602 58.9003 83.6702 55.1503C83.4002 54.6603 83.1203 54.1704 82.8403 53.6804C80.5503 49.7304 78.0902 46.1504 75.4502 42.9404C70.7202 37.1704 65.4203 32.5903 59.5603 29.2103C55.8103 27.0403 52.2902 25.6303 49.0002 24.9703C44.7402 24.1003 40.8704 24.4903 37.4004 26.1603L57.0803 16.3204L57.3704 16.1703C63.5204 13.2103 70.9203 14.2203 79.5603 19.2103C88.6803 24.4703 96.4403 32.6304 102.84 43.6804C109.25 54.7304 112.45 65.4904 112.45 75.9504Z\"\n        stroke=\"currentColor\"\n      />\n    </svg>\n  )\n}\n\nfunction SpacingIcon({ className }: { className?: string }) {\n  return (\n    <svg\n      viewBox=\"0 0 99 125\"\n      fill=\"none\"\n      className={className}\n      strokeWidth=\"1\"\n      strokeLinecap=\"round\"\n      strokeLinejoin=\"round\"\n    >\n      <path\n        d=\"M73.96 53.54L44.27 36.4L29.4299 27.83L22 23.55L2 12V80.14L76.23 122.99V54.85L73.96 53.54ZM24.27 84.47L9.42993 75.9V24.8L22 32.06L24.27 33.37V84.47ZM68.8 110.18L53.96 101.61V50.51L68.8 59.08V110.18Z\"\n        stroke=\"currentColor\"\n      />\n      <path\n        d=\"M96.2295 44.85V112.99L76.2295 122.99V54.85L88.3196 48.8L96.2295 44.85Z\"\n        stroke=\"currentColor\"\n      />\n      <path\n        d=\"M96.23 44.85L88.3201 48.8L76.23 54.85L73.96 53.54L44.27 36.4L29.4299 27.83L22 23.55L2 12L22 2L96.23 44.85Z\"\n        stroke=\"currentColor\"\n      />\n    </svg>\n  )\n}\n\nfunction TypographyIcon({ className }: { className?: string }) {\n  return (\n    <svg\n      viewBox=\"0 0 121 107\"\n      fill=\"none\"\n      className={className}\n      strokeWidth=\"1\"\n      strokeLinecap=\"round\"\n      strokeLinejoin=\"round\"\n    >\n      <path\n        d=\"M27.06 26.5198L22 23.5998L2 12.0498V48.7998L10 53.4198V44.2298L18.53 49.1598L20.68 50.3998V59.5898L28.68 64.2098V27.4598L27.06 26.5198ZM20.68 41.2098L10 35.0498V25.8598L18.53 30.7798L20.68 32.0198V41.2098Z\"\n        stroke=\"currentColor\"\n      />\n      <path\n        d=\"M48.68 17.4598L40.16 21.7198L31.63 25.9898L28.68 27.4598L27.06 26.5198L22 23.5998L2 12.0498L22 2.0498L48.68 17.4598Z\"\n        stroke=\"currentColor\"\n      />\n      <path\n        d=\"M118.08 82.02V94.27L98.0801 104.27V92.02L106.71 87.71L118.08 82.02Z\"\n        stroke=\"currentColor\"\n      />\n      <path\n        d=\"M118.08 57.52V69.77L98.0801 79.77V67.52L99.3901 66.87L109.55 61.79L118.08 57.52Z\"\n        stroke=\"currentColor\"\n      />\n    </svg>\n  )\n}\n\n// Default tools data\nconst pageTools1Defaults = {\n  hero: {\n    label: \"Designer Tools\",\n    title: \"Designer Tools\",\n    description:\n      \"Built to integrate seamlessly with your workflow. All the tools you need in one place.\",\n    primaryCta: { label: \"Read more\", href: \"/tools-information\" },\n    secondaryCta: { label: \"Back Home\", href: \"/\" },\n  },\n  tools: [\n    {\n      slug: \"theme-generator\",\n      title: \"Theme Generator\",\n      description: \"Create custom color themes\",\n      icon: \"theme\",\n    },\n    {\n      slug: \"shadow-composer\",\n      title: \"Shadow Composer\",\n      description: \"Design beautiful box shadows\",\n      icon: \"shadow\",\n    },\n    {\n      slug: \"easing-composer\",\n      title: \"Easing Composer\",\n      description: \"Create custom CSS easing curves\",\n      icon: \"easing\",\n    },\n    {\n      slug: \"spacing-generator\",\n      title: \"Spacing Generator\",\n      description: \"Create consistent spacing scales\",\n      icon: \"spacing\",\n    },\n    {\n      slug: \"typography-composer\",\n      title: \"Typography Composer\",\n      description: \"Design type scales and pairings\",\n      icon: \"typography\",\n    },\n  ],\n}\n\n// Tool Icon Renderer\nfunction ToolIcon({\n  icon,\n  className,\n}: {\n  icon: string\n  className?: string\n}) {\n  switch (icon) {\n    case \"theme\":\n      return <ThemeIcon className={className} />\n    case \"shadow\":\n      return <ShadowIcon className={className} />\n    case \"easing\":\n      return <EasingIcon className={className} />\n    case \"spacing\":\n      return <SpacingIcon className={className} />\n    case \"typography\":\n      return <TypographyIcon className={className} />\n    default:\n      return <ThemeIcon className={className} />\n  }\n}\n\n// Tool Card Component\nfunction ToolCard({\n  tool,\n  baseUrl = \"/tools\",\n}: {\n  tool: (typeof pageTools1Defaults.tools)[0]\n  baseUrl?: string\n}) {\n  return (\n    <Link\n      href={`${baseUrl}/${tool.slug}`}\n      className=\"group bg-background relative flex flex-col items-center rounded-2xl border px-6 pb-6 text-center shadow-sm\"\n    >\n      <ArrowUpRight className=\"text-muted-foreground group-hover:text-primary absolute top-4 right-4 size-5 transition-colors\" />\n      <div className=\"text-primary py-8\">\n        <ToolIcon icon={tool.icon} className=\"size-32\" />\n      </div>\n      <h3 className=\"text-lg font-semibold\">{tool.title}</h3>\n      <p className=\"text-muted-foreground text-sm\">{tool.description}</p>\n    </Link>\n  )\n}\n\n// Striped Background Component\nfunction StripedBackground({\n  columnCount = 6,\n  className,\n}: {\n  columnCount?: number\n  className?: string\n}) {\n  return (\n    <div\n      className={cn(\n        \"pointer-events-none absolute inset-0 z-0 overflow-hidden\",\n        className\n      )}\n    >\n      <div className=\"container relative mx-auto h-full px-6\">\n        <div className=\"absolute inset-0 flex justify-between\">\n          {Array.from({ length: columnCount + 1 }).map((_, i) => (\n            <div key={i} className=\"bg-border/30 h-full w-px\" />\n          ))}\n        </div>\n      </div>\n    </div>\n  )\n}\n\ninterface PageTools1Props {\n  hero?: Partial<typeof pageTools1Defaults.hero>\n  tools?: typeof pageTools1Defaults.tools\n  baseUrl?: string\n  className?: string\n}\n\nexport function PageTools1({\n  hero = {},\n  tools = pageTools1Defaults.tools,\n  baseUrl = \"/tools\",\n  className,\n}: PageTools1Props) {\n  const heroContent = { ...pageTools1Defaults.hero, ...hero }\n\n  return (\n    <div className={cn(\"relative -mt-14 overflow-x-clip\", className)}>\n      <StripedBackground columnCount={6} />\n\n      <div className=\"relative z-10 container px-6\">\n        <Spacer size=\"6xl\" sizeMobile=\"4xl\" />\n\n        <div className=\"mx-auto max-w-4xl\">\n          {/* Hero Section */}\n          <div className=\"flex flex-col items-center text-center\">\n            <p className=\"text-primary mb-4 text-sm font-medium tracking-widest uppercase\">\n              {heroContent.label}\n            </p>\n            <h1 className=\"text-4xl font-bold tracking-tight sm:text-5xl md:text-6xl\">\n              {heroContent.title}\n            </h1>\n            <Spacer size=\"lg\" sizeMobile=\"md\" />\n            <p className=\"text-muted-foreground text-lg md:text-xl\">\n              {heroContent.description}\n            </p>\n            <Spacer size=\"xl\" sizeMobile=\"lg\" />\n            <div className=\"flex flex-row gap-2\">\n              {heroContent.primaryCta && (\n                <Link href={heroContent.primaryCta.href}>\n                  <HeroButton size=\"default\">\n                    {heroContent.primaryCta.label}\n                  </HeroButton>\n                </Link>\n              )}\n              {heroContent.secondaryCta && (\n                <Link href={heroContent.secondaryCta.href}>\n                  <Button\n                    variant=\"outline\"\n                    size=\"default\"\n                    className=\"rounded-full\"\n                  >\n                    {heroContent.secondaryCta.label}\n                  </Button>\n                </Link>\n              )}\n            </div>\n          </div>\n\n          <Spacer size=\"4xl\" sizeMobile=\"2xl\" />\n\n          {/* Tools Grid */}\n          <div className=\"grid grid-cols-1 gap-4 sm:grid-cols-2 lg:grid-cols-3\">\n            {tools.map((tool) => (\n              <ToolCard key={tool.slug} tool={tool} baseUrl={baseUrl} />\n            ))}\n          </div>\n        </div>\n\n        <Spacer size=\"6xl\" sizeMobile=\"4xl\" />\n      </div>\n    </div>\n  )\n}\n",
      "type": "registry:block"
    }
